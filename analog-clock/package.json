{
  "name": "analog-clock",
  "scripts": {
    "postinstall": "jspm install -y",
    "start": "node app.js",
    "lint": "standard --verbose | snazzy",
    "watch:js": "jspm -w",
    "watch": "concurrently --kill-others 'npm run lint' 'npm run watch:js'"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "jspm": "^0.17.0-beta.13",
    "snazzy": "^3.0.1",
    "standard": "^6.0.8"
  },
  "dependencies": {
    "express": "^4.13.4"
  }
}
